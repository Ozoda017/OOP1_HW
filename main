package less1;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;


public class Main {
    public static void main(String[] args) {
        VendingMachine vendingMachine = new VendingMachine();
        List<Product> products = new ArrayList<>(Arrays.asList(new Product("coffee", 30),
                new Product("tea", 50), new Product("chocolate", 65)));
        vendingMachine.intProduct(products);
        System.out.println(vendingMachine.getProduct("tea").toString());


    }
}